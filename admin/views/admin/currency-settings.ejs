<%- include("partials/header.ejs") %> 

<!-- Content Header (Page header) -->
<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">Currency Settings</h1>
      </div>
      <!-- /.col -->
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="#">Home</a></li>
          <li class="breadcrumb-item active">Currency Settings</li>
        </ol>
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
  <div class="container-fluid">
    <!-- Small boxes (Stat box) -->
    <div class="row">
      <div class="col-md-12">
        <!-- general form elements disabled -->
        <div class="card card-warning">
          <div class="card-header">
            <h3 class="card-title">Currency Settings</h3>
          </div>
          <!-- /.card-header -->
          <div class="card-body">
            <form method="post" action="/admin/currency-settings" id="frm-currency-settings">
              <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                      <label>Select Status</label>
                      <select name="dd_currency" class="form-control">
                        <option value="">Choose Currency</option>
                        <option value="USD">USD(United State Dollar)</option>
                        <option value="INR">INR(indian Rupee)</option>
                        <option value="EUR">EUR(EURO)</option>
                      </select>
                    </div>
                 </div>
                </div>
              <button class="btn btn-dark" type="submit">Submit</button>
            </form>

          </div>
          <!-- /.card-body -->
        </div>
      </div>
    </div>
  </div>
  <!-- /.container-fluid -->
</section>
<!-- /.content -->
<%- include("partials/footer.ejs") %> 
